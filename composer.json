{
	"name" : "mostafabarmshory/wp",
	"description" : "Clone wordpress site with no access",
	"type" : "library",
	"license" : "MIT",
	"homepage" : "https://github.com/pluf/",
	"authors" : [{
			"name" : "Mostafa Barmshory",
			"email" : "mostafa.barmshory@gmail.com"
		}
	],
	"support" : {
		"email" : "info@pluf.ir",
		"issues" : "https://github.com/mostafabarmshory/wp",
		"forum" : "https://github.com/mostafabarmshory/wp",
		"wiki" : "https://github.com/mostafabarmshory/wp",
		"source" : "https://github.com/mostafabarmshory/wp"
	},
	"autoload" : {
		"psr-4" : {
			"Pluf\\WP\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Pluf\\Tests\\WP\\" : "tests"
		}
	},
	"keywords" : [
		"WP",
		"wordpress"
	],
	"bin" : [
		"bin/wp-clone",
		"bin/wp-clean",
		"bin/wp-update",
		"bin/wp-removeStyle",
		"bin/wp-minify",
		"bin/wp-upload"
	],
	"require" : {
		"pluf/log" : "~7.0",
		"pluf/scion" : "~7.0",
		"guzzlehttp/guzzle" : "^7.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~9.5"
	},
	"config" : {
		"platform" : {
			"php" : "^8.0.1"
		}
	}
}